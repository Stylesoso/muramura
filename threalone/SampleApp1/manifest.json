    		"assets/live2d/haru/haru_02.1024/*",
    		"assets/live2d/haru/motions/*",
    		"assets/live2d/haru/sounds/*",
    		"assets/live2d/murakumo/*",
    		"assets/live2d/murakumo/expressions/*",
    		"assets/live2d/murakumo/murakumo.1024/*",
    		"assets/live2d/murakumo/motions/*",
    		"assets/live2d/murakumo/sounds/*"

    ]

}





         
