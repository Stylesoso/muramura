{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": [
    		"src/jquery-2.1.4.js",
           	"src/live2d.min.js",
        	"src/Live2DFramework.js",
        	"src/utils/MatrixStack.js",
        	"src/utils/ModelSettingJson.js",
        	"src/PlatformManager.js",
        	"src/LAppDefine.js",
        	"src/LAppModel.js",
        	"src/LAppLive2DManager.js",
        	"src/SampleApp1.js"]

  }
	],
	"web_accessible_resources": [
    		"assets/live2d/haru/*",
    		"assets/live2d/haru/expressions/*",
